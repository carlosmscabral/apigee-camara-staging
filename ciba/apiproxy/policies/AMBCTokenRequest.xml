<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AMBCTokenRequest">
    <DisplayName>AM.BCTokenRequest</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>flow.grantType</Name>
        <Value>authorization_code</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>flow.redirectURI</Name>
        <Value>https://localhost</Value>
